{
  "name": "custom-pointer",
  "displayName": "Custom Pointer",
  "description": "Custom mouse pointer/cursor for Visual Studio Code",
  "icon": "logo.png",
  "version": "1.2.1",
  "publisher": "diegocurbelo",
  "author": {
    "email": "diego@curbelo.com",
    "name": "Diego Curbelo",
    "url": "https://diego.curbelo.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/diegocurbelo/vscode-custom-pointer.git"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.28.1"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./src/extension",
  "contributes": {
    "commands": [
      {
        "command": "customPointer.enable",
        "title": "Custom Pointer: Enable"
      },
      {
        "command": "customPointer.disable",
        "title": "Custom Pointer: Disable"
      },
      {
        "command": "customPointer.remove",
        "title": "Custom Pointer: Remove"
      }
    ]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.0",
    "eslint": "^5.7.0",
    "typescript": "^3.1.3",
    "vscode": "^1.1.6"
  }
}
